package teat.java;
import java.util.*;

public class Sum {

	public static void main(String[] args) {
		System.out.println("enter number");
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int d=0;
		for(int i=a;i>0;i--) {
			d=d+i;
		}
		for(int j=a;j>0;j--) {
		System.out.println(j);
		}

	}

}
