package teat.java;
import java.util.*;
public class Array2 {

	public static void main(String[] args) {
		System.out.println("Enter Length of Array:-");
		Scanner sc=new Scanner(System.in);
		int s=sc.nextInt();
		int a[]=new int[s];
		int count=0;
		for(int i=0;i<a.length;i++) {
			System.out.println("Enter number:-");
			 a[i]=sc.nextInt();
		}

	}

}
